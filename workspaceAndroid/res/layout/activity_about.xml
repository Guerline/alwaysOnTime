<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:ads="http://schemas.android.com/apk/res-auto"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	
>
<RelativeLayout 
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
	<TextView android:id="@+id/activity_aboutTextView_title"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:textColor="#1a70ad"
		android:textAppearance="@android:style/TextAppearance.Large"
		android:textStyle="bold" />

	<TextView android:id="@+id/activity_aboutTextView"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:textColor="#fff"
		android:layout_below="@id/activity_aboutTextView_title"
		android:textAppearance="@android:style/TextAppearance.Medium" />
	
	<RelativeLayout 
		android:id="@+id/activity_aboutRelativeLayout1"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:gravity="center_horizontal"
		android:layout_below="@+id/activity_aboutTextView"
    >	
	
    	<TextView android:id="@+id/activity_aboutTextView1"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:textColor="#fff"
			android:textAppearance="@android:style/TextAppearance.Medium" />

	<ImageView android:id="@+id/activity_aboutImageView1"
		
		android:layout_width="@dimen/about_imageview_width"
		android:layout_height="wrap_content"
		android:paddingLeft="@dimen/about_padding_left"
		android:layout_below="@+id/activity_aboutTextView1"
		android:gravity="center_horizontal"
		/>
	
		<ImageView android:id="@+id/activity_aboutImageView12"
			android:layout_width="300dp"
	
			android:layout_height="wrap_content"
			android:paddingLeft="@dimen/about_padding_left"
			android:layout_below="@+id/activity_aboutImageView1"
			android:gravity="center_horizontal"
		/>
		
		
	</RelativeLayout>

<RelativeLayout 
	android:id="@+id/activity_aboutRelativeLayout2"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:layout_below="@+id/activity_aboutRelativeLayout1"
    >	
	
	<TextView android:id="@+id/activity_aboutTextView2"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:textColor="#fff"
		android:textAppearance="@android:style/TextAppearance.Medium" />
	<ImageView android:id="@+id/activity_aboutImageView2"
		android:layout_width="@dimen/about_imageview_width"
		android:layout_height="wrap_content"
		android:paddingLeft="@dimen/about_padding_left"
		android:layout_below="@+id/activity_aboutTextView2"
		android:gravity="center_horizontal"
	/>
</RelativeLayout>

	<RelativeLayout 
	android:id="@+id/activity_aboutRelativeLayout3"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
		android:layout_below="@+id/activity_aboutRelativeLayout2"
    >

	<TextView android:id="@+id/activity_aboutTextView3"
		android:layout_width="fill_parent"
		android:layout_height="wrap_content"
		android:textColor="#fff"
		android:textAppearance="@android:style/TextAppearance.Medium" />
	
		<ImageView android:id="@+id/activity_aboutImageView31"
			android:layout_width="@dimen/about_imageview_width"
			android:layout_height="wrap_content"
			android:paddingLeft="@dimen/about_padding_left"
			android:layout_below="@+id/activity_aboutTextView3"
			android:gravity="center_horizontal"
		/>
	
		<ImageView android:id="@+id/activity_aboutImageView32"
			
			android:layout_width="@dimen/about_imageview_width"
			android:layout_height="wrap_content"
			android:paddingLeft="@dimen/about_padding_left"
			android:layout_below="@+id/activity_aboutImageView31"
			android:gravity="center_horizontal"
		/>
	</RelativeLayout>
	
	<RelativeLayout 
		android:id="@+id/activity_aboutRelativeLayout4"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:layout_below="@+id/activity_aboutRelativeLayout3"
    >	
		<TextView android:id="@+id/activity_aboutTextView4"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:textColor="#fff"
			android:textAppearance="@android:style/TextAppearance.Medium" />

		<ImageView android:id="@+id/activity_aboutImageView41"
			android:layout_width="@dimen/about_imageview_width"
			android:layout_height="wrap_content"
			android:paddingLeft="@dimen/about_padding_left"
			
			android:layout_below="@+id/activity_aboutTextView4"
			android:gravity="center_horizontal"
		/>

		<ImageView android:id="@+id/activity_aboutImageView42"
			android:layout_width="@dimen/about_imageview_width"
			android:layout_height="wrap_content"
			android:paddingLeft="@dimen/about_padding_left"
			android:layout_below="@+id/activity_aboutImageView41"
			android:gravity="center_horizontal"
		/>
	</RelativeLayout>
	
	<RelativeLayout 
		android:id="@+id/activity_aboutRelativeLayout5"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:layout_below="@+id/activity_aboutRelativeLayout4"
    >	
		<TextView android:id="@+id/activity_aboutTextView5"
			android:layout_width="fill_parent"
			android:layout_height="wrap_content"
			android:textColor="#fff"
			android:textAppearance="@android:style/TextAppearance.Medium" />

		<ImageView android:id="@+id/activity_aboutImageView51"
			android:layout_width="@dimen/about_imageview_width"
			android:layout_height="wrap_content"
			android:paddingLeft="@dimen/about_padding_left"

			android:layout_below="@+id/activity_aboutTextView5"
			android:gravity="center_horizontal"
		/>

		<ImageView android:id="@+id/activity_aboutImageView52"
			android:layout_width="@dimen/about_imageview_width"
			android:layout_height="wrap_content"
			android:paddingLeft="@dimen/about_padding_left"
			android:layout_below="@+id/activity_aboutImageView51"
			android:gravity="center_horizontal"
		/>
	</RelativeLayout>
	</RelativeLayout>
	</ScrollView>


